{
  "name": "nextjs-expense-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:init": "dotenv -e .env.local -- pnpm prisma migrate dev --name init && pnpm prisma db seed",
    "db:reset": "dotenv -e .env.local -- pnpm prisma migrate reset --force"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@prisma/client": "5.16.2",
    "@tailwindcss/forms": "^0.5.7",
    "clsx": "^2.1.1",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prisma": "^5.16.2",
    "tailwindcss": "^3.4.1"
  },
  "prisma": {
    "seed": "dotenv -e .env.local -- node prisma/seed.js"
  }
}
